# LocalComamnd screen -dR

## Dynamic jumphost
## i.e. ssh util01+ndh1-vb01exs033.mckesson
Host *+*
    ## Netcat
    ProxyCommand ssh $(echo %h | sed 's/+[^+]*$//;s/\([^+%%]*\)%%\([^+]*\)$/\2 -l \1/;s/:/ -p /') exec ncat -w1 $(echo %h | sed 's/^.*+//;/:/!s/$/ %p/;s/:/ /')
    ## socat
    #ProxyCommand ssh $(echo %h | sed 's/+[^+]*$//;s/\([^+%%]*\)%%\([^+]*\)$/\2 -l \1/;s/:/ -p /') exec ncat STDIN TCP:$(echo %h | sed 's/^.*+//;/:/!s/$/ %p/;s/:/ /')

## McKesson
Host *.ipcenter.com
    User jhafner

Host *.mckesson
    User ez6a8vt
    ProxyCommand ssh -q util01 ncat -w1 %h %p

Host util01
    HostName util01.prod.mkesson.ipcenter.com


